{
    "repositories": [
	{
	    "type": "package",
	    "package": {
		"name": "wordpress",
		"type": "webroot",
		"version": "4.7",
		"dist": {
		    "type": "zip",
		    "url": "https://wordpress.org/wordpress-4.7.zip"
		},
		"require" : {
		    "fancyguy/webroot-installer": "1.0.0"
		}
	    }
	},
	{
	    "type":"composer",
            "url":"https://wpackagist.org"
	}
    ],
    "require": {
	"php": ">=5.5.9",
	"wordpress": "4.6.1",
	"fancyguy/webroot-installer": "1.0.0",
	"wpackagist-plugin/advanced-sidebar-menu": "~6.1.5",
	"wpackagist-plugin/akismet":"3.2",
        "wpackagist-plugin/all-in-one-wp-security-and-firewall":"~4.2.4",
	"wpackagist-plugin/better-search-replace":"~1.3",
	"wpackagist-plugin/breadcrumb-navxt": "~5.5.1",
	"wpackagist-plugin/cas-maestro": "1.1.3",
	"wpackagist-plugin/co-authors-plus":"*",
	"wpackagist-plugin/easy-add-thumbnail": "1.1.1",
	"wpackagist-plugin/google-analytics-for-wordpress":"~5.5.2",
	"wpackagist-plugin/gravity-forms-google-analytics-event-tracking": "2.0.3",
	"wpackagist-plugin/jetpack":"~4.4.2",
        "wpackagist-plugin/popup-maker": "~1.4.21",
	"wpackagist-plugin/regenerate-thumbnails": "2.2.6",
	"wpackagist-plugin/simple-local-avatars": "2.0",
	"wpackagist-plugin/widget-css-classes": "1.4.0",
	"wpackagist-plugin/wordpress-importer":"0.6.3",
	"wpackagist-plugin/wordpress-seo":"4.0.2",
	"wpackagist-plugin/wp-super-cache":"1.4.8",
	"wpackagist-theme/twentysixteen": "1.2"
    },
    "extra": {
      "webroot-dir": "docroot/wordpress",
      "webroot-package": "wordpress",
      "installer-paths": {
        "docroot/wp-content/plugins/{$name}/": ["type:wordpress-plugin"],
        "docroot/wp-content/themes/{$name}/": ["type:wordpress-theme"]
      }
    }
}
